{
  "name": "nestjs-route-exporter",
  "version": "1.0.3",
  "description": "Nest route exporter, exports application routes with developer friendly interface for consuming them on FE apps or somewhere else.",
  "author": "Djordje Stojiljkovic <djordjestojilljkovic@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write",
    "lint": "eslint 'src/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:integration": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:integration:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@nestjs/common": "7.3.2",
    "@nestjs/core": "7.3.2",
    "@types/jest": "26.0.5",
    "@types/node": "12.12.31",
    "class-transformer": "0.2.3",
    "class-validator": "0.14.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "reflect-metadata": "0.1.13",
    "release-it": "12.6.3",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "26.1.3",
    "ts-morph": "7.0.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "reflect-metadata": "^0.1.12"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "files": ["lib/**/*"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unckleg/nestjs-route-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/unckleg/nestjs-route-exporter/issues"
  },
  "homepage": "https://github.com/unckleg/nestjs-route-exporter#readme"
}
